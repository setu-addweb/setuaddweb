  (function(){
    // impression & click tracking values
    let trackingContentPosition = 'alertBanner';
    let trackingOfferType = 'content'; /* booking, loyalty, cobrand, hvmi, app */
    let trackingDescription = 'covid1120';
    let trackingTag = 'US-nonHP';
    let atBannerViewCount = 1;

    let atCTAclickTrackVal = 'Banner|' + trackingDescription + '|external|event167,list3=' + trackingContentPosition + ';type=' + trackingOfferType + ';msg=' + trackingDescription + ';tag=' + trackingTag;

    function getCookie(cname) {
      var name = cname + "=";
      var ca = document.cookie.split(';');
      for(var i = 0; i < ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) == ' ') {
          c = c.substring(1);
        }
        if (c.indexOf(name) == 0) {
          return c.substring(name.length, c.length);
        }
      }
      return "";
    }

    function setCookie(cname, cvalue, minutes) {
      var d = new Date();
      d.setTime(d.getTime() + minutes * 60 * 1000);
      var expires = "expires=" + d.toUTCString();
      document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
    }

    function atBannerUpdate() {
      // updates banner template update
      let atBannerTemplate = '<div id="atAlertBanner" class="is-hidden" role="alert"><div class="l-container"><div class="t-icon-bestrate" id="at-alert-banner-peace-of-mind-mob"><div class="at-alert-banner-lead-copy"><div class="at-alert-banner-lead-copy-header">Best Rate Guarantee</div><div class="at-alert-banner-lead-copy-cta"><a class="custom_click_track" custom_click_track_value="' + atCTAclickTrackVal + 'guarantee" href="https://clean.marriott.com/#bestrate">Travel with confidence during COVID-19</a>.</div></div></div><div class="l-center-align l-m-display-block" id="at-alert-banner-peace-of-mind-desk"><div class="at-alert-banner-lead-copy"><span>Travel with confidence</span> <span>during COVID-19:</span></div><div class="at-alert-banner-icon-container"><div class="at-alert-banner-pay-direct-container at-icon-1"><a class="custom_click_track" custom_click_track_value="' + atCTAclickTrackVal + 'guarantee" href="https://clean.marriott.com/#bestrate"><span class="t-color-standard-80 t-icon-bestrate">&nbsp;</span> <span class="t-font-weight-semibold">Best Rate Guarantee</span></a></div><div class="at-alert-banner-pay-direct-container at-icon-2"><a class="custom_click_track" custom_click_track_value="' + atCTAclickTrackVal + 'cancellation" href="https://clean.marriott.com/#flexiblecancellation"><span class="t-color-standard-80 t-icon-cancel">&nbsp;</span> <span class="t-font-weight-semibold">Flexible Cancellation</span></a></div><div class="at-alert-banner-pay-direct-container at-icon-3"><a class="custom_click_track" custom_click_track_value="' + atCTAclickTrackVal + 'clean" href="https://clean.marriott.com/"><span class="t-color-standard-80 t-icon-clean">&nbsp;</span> <span class="t-font-weight-semibold">Commitment to Clean</span></a></div><div class="at-alert-banner-pay-direct-container at-icon-4"><a class="custom_click_track" custom_click_track_value="' + atCTAclickTrackVal + 'travelguidance" href="https://travelguidance.marriott.com"><span class="t-color-standard-80 t-icon-globe-tick">&nbsp;</span> <span class="t-font-weight-semibold">Travel Guidance</span></a></div></div></div></div> <button aria-label="Close" class="atAlertBannerCloseTippy"></button></div>';
      var refNode = document.querySelector('#page-container');
      refNode.insertAdjacentHTML("afterbegin", atBannerTemplate);

      var mtRandomBanner = Math.floor((Math.random() * 4) + 1);
      switch (mtRandomBanner) {
        case 2:
          document.getElementById('at-alert-banner-peace-of-mind-mob').classList.remove("t-icon-bestrate");
          document.getElementById('at-alert-banner-peace-of-mind-mob').classList.add("t-icon-cancel");
          document.querySelector('.at-alert-banner-lead-copy-header').innerText = "Flexible Cancellation";
          document.querySelector('.at-alert-banner-lead-copy-cta a').setAttribute("href", "https://clean.marriott.com/#flexiblecancellation");
          document.querySelector('.at-alert-banner-lead-copy-cta a').setAttribute("custom_click_track_value", atCTAclickTrackVal + 'cancellation');
          break;
        case 3:
          document.getElementById('at-alert-banner-peace-of-mind-mob').classList.remove("t-icon-bestrate");
          document.getElementById('at-alert-banner-peace-of-mind-mob').classList.add("t-icon-clean");
          document.querySelector('.at-alert-banner-lead-copy-header').innerText = "Commitment to Clean";
          document.querySelector('.at-alert-banner-lead-copy-cta a').setAttribute("href", "https://clean.marriott.com/");
          document.querySelector('.at-alert-banner-lead-copy-cta a').setAttribute("custom_click_track_value", atCTAclickTrackVal + 'clean');
          break;
        case 4:
          document.getElementById('at-alert-banner-peace-of-mind-mob').classList.remove("t-icon-bestrate");
          document.getElementById('at-alert-banner-peace-of-mind-mob').classList.add("t-icon-globe-tick");
          document.querySelector('.at-alert-banner-lead-copy-header').innerText = "Travel Guidance";
          document.querySelector('.at-alert-banner-lead-copy-cta a').setAttribute("href", "https://travelguidance.marriott.com");
          document.querySelector('.at-alert-banner-lead-copy-cta a').setAttribute("custom_click_track_value", atCTAclickTrackVal + 'travelguidance');
          break;
        default:
      }
      document.querySelectorAll(".atAlertBannerCloseTippy")[0].onclick = function() { atSConC(); };
      document.getElementById('atAlertBanner').style.cssText = "";
    }

    function atSConC() {
      document.getElementById('atAlertBanner').style.display = "none";        
      let cname = "merchViewed";
      let cvalue = trackingContentPosition + "-" + trackingDescription + "-closed"
      let minutes = 30;
      let ctemp = getCookie(cname) || "";
      cvalue = ctemp + cvalue + '|';
      setCookie(cname, cvalue, minutes);
    }

    var atCallback = function () {
      // merchandising impression tracking
      let cname = "merchViewed";
      let cvalue = trackingContentPosition + "-" + trackingDescription + "-" + atBannerViewCount;
      let minutes = 30;
      let ctemp = getCookie(cname) || "";
      if (ctemp.length > 0) {
        if (ctemp.includes(trackingContentPosition) == true) {
          cvalue = ctemp.split('|');
          cvalue.forEach(atPlus1);
          function atPlus1(value, index) {
            if (value.includes('alertBanner')) {
              let z = parseInt(cvalue[index].split('-')[2]);
              z++;
              cvalue[index] = cvalue[index].split('-')[0] + '-' + cvalue[index].split('-')[1] + '-' + z;
            }
          };
          cvalue = cvalue.join('|');
        } 
        else {
          cvalue = ctemp + cvalue + "|";
        }
      }
      else {
        cvalue = cvalue + "|";
      }
      setCookie(cname, cvalue, minutes);
      impressionTrack('event168|list3=' + trackingContentPosition + ',type=' + trackingOfferType + ',msg=' + trackingDescription + ',tag=' + trackingTag);
      setTimeout(function(){
        atBannerUpdate();
        $('#atAlertBanner').removeClass('is-hidden');
      }, 1000);
    };

    if ( document.readyState === "complete" || (document.readyState !== "loading" && !document.documentElement.doScroll)) {
      atCallback();      
    } else {
      document.addEventListener("DOMContentLoaded", atCallback);
    }
  })();
